{
  "name": "vibesonly",
  "version": "1.0.0",
  "description": "Voice-based workforce skills assessment platform",
  "private": true,
  "type": "module",
  "main": "dist/server.js",
  "engines": {
    "node": ">=24.0.0"
  },
  "scripts": {
    "build": "tsc",
    "migrate": "node-pg-migrate up --database-url-var DATABASE_URL --migrations-dir migrations",
    "migrate:down": "node-pg-migrate down --database-url-var DATABASE_URL --migrations-dir migrations",
    "start": "npm run migrate && npm run build && node dist/server.js",
    "start:no-migrate": "node dist/server.js",
    "start:built": "npm run migrate && node dist/server.js",
    "setup-whisper": "echo 'Whisper is set up automatically during Docker build'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.44",
    "ai": "^6.0.86",
    "axios": "^1.13.5",
    "cors": "^2.8.5",
    "dotenv": "^17.3.1",
    "express": "^5.2.1",
    "multer": "^2.0.2",
    "node-pg-migrate": "^8.0.4",
    "pg": "^8.18.0",
    "pg-boss": "^12.12.0",
    "pg-workflows": "^0.1.1",
    "whisper-node": "^1.1.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.6",
    "@types/multer": "^2.0.0",
    "@types/node": "^25.2.3",
    "@types/pg": "^8.11.10",
    "typescript": "^5.7.0"
  }
}
